{
  "package": "com.cljiyunjb.fortunecljy",
  "name": "吉运计步",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1121,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.webview"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "demo-template",
  "config": {
    "logLevel": "debug",
    "designWidth": 250
  },
  "subpackages": [
    {
      "name": "pkg_main",
      "resource": "pkg_main"
    }
  ],
  "router": {
    "entry": "pkg_main/pages/Demo",
    "pages": {
      "pkg_main/pages/Demo": {
        "component": "index"
      },
      "pkg_main/pages/DemoDetail": {
        "component": "index"
      },
      "pkg_main/pages/Web": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "pkg_main/pages/Demo": {
        "titleBar": false,
        "menu": false
      },
      "pkg_main/pages/DemoDetail": {
        "titleBarText": "详情页"
      },
      "pkg_main/pages/Web": {
        "titleBarText": ""
      }
    }
  },
  "deviceTypeList": ["phone"]
}
